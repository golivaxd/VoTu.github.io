{
  "version": 2,
  "builds": [
    {
      "src": "B/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "F/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "B/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "F/$1"
    }
  ]
}
